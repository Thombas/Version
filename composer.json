{
    "name": "thomasfielding/version",
    "description": "A package for storing, calculating and updating the version number and history of your application.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Thomas Fielding",
            "email": "thombas.fielding@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^8.0|^9.0",
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "ThomasFielding\\Version\\Tests\\": "tests/",
            "ThomasFielding\\Version\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThomasFielding\\Version\\Providers\\VersionServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    }
}
